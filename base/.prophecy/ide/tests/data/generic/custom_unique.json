{
  "custom_unique" : {
    "name" : "custom_unique",
    "definition" : "\n\nWITH dup AS (\n    SELECT {{ column_name }}, COUNT(*) AS count\n    FROM {{ model }}\n    GROUP BY {{ column_name }}\n    HAVING COUNT(*) > 1\n)\n\nSELECT * FROM dup",
    "parameters" : {
      "type" : "record",
      "fields" : [ {
        "name" : "model",
        "kind" : {
          "type" : "table"
        },
        "optional" : false,
        "isWorkflowNodeConfiguration" : false,
        "isReferenced" : false
      }, {
        "name" : "column_name",
        "kind" : {
          "type" : "column"
        },
        "optional" : false,
        "isWorkflowNodeConfiguration" : false,
        "isReferenced" : false
      } ]
    },
    "staleState" : "none",
    "version" : 0
  }
}